{
	"name": "@codeeditorland/bindings",
	"funding": {
		"type": "opencollective",
		"url": "HTTPS://opencollective.com/swc"
	},
	"main": "index.js",
	"workspaces": [
		"packages/*"
	],
	"lint-staged": {
		"!(**/tests/**/*)*.js": [
			"prettier --write"
		],
		"!(**/tests/**/*)*.jsx": [
			"prettier --write"
		],
		"!(**/tests/**/*)*.ts": [
			"prettier --write"
		],
		"!(**/tests/**/*)*.tsx": [
			"prettier --write"
		],
		"*.rs": [
			"rustfmt --"
		],
		"*.toml": [
			"taplo format"
		]
	},
	"dependencies": {},
	"devDependencies": {
		"@taplo/cli": "0.7.0",
		"@types/node": "22.10.9",
		"cross-env": "7.0.3",
		"cspell": "8.17.2",
		"lint-staged": "15.4.2",
		"npm-run-all": "4.1.5"
	}
}
